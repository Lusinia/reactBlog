{
  "name": "BlogPosts",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "concurrently \"yarn server\" \"yarn android\"",
    "test": "jest",
    "link": "node node_modules/react-native/local-cli/cli.js link",
    "lint": "./node_modules/.bin/eslint src --fix",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "build:ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file='index.js' --bundle-output='./ios/ultraf/main.jsbundle' --dev=true --platform='ios' --assets-dest='./ios'",
    "build:android": "chmod +x ./bin/run.sh && ./bin/run.sh",
    "server": "http-server -p 8000  ./public --cors"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-react": "^7.8.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-cached-image": "^1.4.3",
    "react-native-config": "^0.11.5",
    "react-native-fast-image": "^4.0.14",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-img-cache": "^1.5.3",
    "react-native-navigation": "^1.1.465",
    "react-native-progress": "^3.5.0",
    "react-native-segment-controller": "^1.0.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "23.0.1",
    "babel-preset-react-native": "4.0.0",
    "http-server": "^0.11.1",
    "jest": "23.1.0",
    "react-test-renderer": "16.3.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "resolutions": {
    "uglify-es": "3.2.2"
  }
}
